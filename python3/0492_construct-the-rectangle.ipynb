    "from typing import List\n",
    "class Solution:\n",
    "    def constructRectangle(self, area: int) -> List[int]:\n",
    "        side = int(area ** 0.5)\n",
    "        while area % side != 0:\n",
    "            side -= 1\n",
    "        return [int(area/side), side]\n",
    "        "
   ]
