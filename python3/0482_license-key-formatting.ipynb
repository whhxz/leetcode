   "source": [
    "class Solution:\n",
    "    def licenseKeyFormatting(self, S: str, K: int) -> str:\n",
    "        ss = \"\".join(S[::-1].split('-'))\n",
    "        return \"-\".join([ss[i:i+K].upper() for i in range(0, len(ss), K)])[::-1]"
   ]
  },
