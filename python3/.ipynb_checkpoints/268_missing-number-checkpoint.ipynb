    "class Solution:\n",
    "    def missingNumber(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        l = len(nums)\n",
    "        #数学公式 f(n) =(1+n)*n/2\n",
    "        return int(l * (l + 1) / 2) - sum(nums)"
   ]
