   "source": [
    "class Solution:\n",
    "    def frequencySort(self, s: str) -> str:\n",
    "        sm = {}\n",
    "        for c in s:\n",
    "            sm.setdefault(c, 0)\n",
    "            sm[c] += 1\n",
    "        return \"\".join([c * sm.get(c) for c in sorted(sm, key=sm.get, reverse=True)])"
   ]
  },
