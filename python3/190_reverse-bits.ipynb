   "source": [
    "class Solution:\n",
    "    # @param n, an integer\n",
    "    # @return an integer\n",
    "    def reverseBits(self, n):\n",
    "        tmp = bin(n)[2:]\n",
    "        tmp = '0' * (32 - len(tmp)) + tmp\n",
    "        return int(tmp[::-1], 2)"
   ]
