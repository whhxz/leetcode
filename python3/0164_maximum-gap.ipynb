   "source": [
    "from typing import List\n",
    "class Solution:\n",
    "    def maximumGap(self, nums: List[int]) -> int:\n",
    "        nums.sort()\n",
    "        res = 0\n",
    "        for i in range(1, len(nums)):\n",
    "            res = max(nums[i] - nums[i-1], res)\n",
    "        return res"
   ]
