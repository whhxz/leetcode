   "source": [
    "class Solution:\n",
    "    def addDigits(self, num: int) -> int:\n",
    "        if num <10:\n",
    "            return num\n",
    "        res = 0\n",
    "        for i in str(num):\n",
    "            res += int(i)\n",
    "        return self.addDigits(res)"
   ]
