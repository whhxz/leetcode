   "source": [
    "class Solution:\n",
    "    #去掉最后一个1\n",
    "    def countBits(self, num: int) -> List[int]:\n",
    "        res = [0]\n",
    "        for i in range(1, num+1):\n",
    "            #当前i右移，计算有多少位，加上消掉的最后一位是否为1，动态规划\n",
    "            res.append(res[i>>1]+(i&1))\n",
    "        return res"
   ]
  }
