   "source": [
    "from typing import List\n",
    "class Solution:\n",
    "    #没错n-1个元素加1，可以视为某个元素减1\n",
    "    def minMoves(self, nums: List[int]) -> int:\n",
    "        min_num = min(nums)\n",
    "        return sum([num - min_num for num in nums])"
   ]
  }
