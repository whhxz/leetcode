    "from typing import List\n",
    "\n",
    "\n",
    "class Solution:\n",
    "    def distributeCandies(self, candies: List[int]) -> int:\n",
    "        s = set(candies)\n",
    "        return len(s) if len(candies)//2 > len(s) else len(candies)//2\n"
   ]
