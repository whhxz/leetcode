    "class Solution:\n",
    "    def canWinNim(self, n: int) -> bool:\n",
    "        if n == 1 or n == 2 or n == 3:\n",
    "            return True\n",
    "        return self.canWinNim(n-1) or self.canWinNim(n-2) or self.canWinNim(n-3)"
