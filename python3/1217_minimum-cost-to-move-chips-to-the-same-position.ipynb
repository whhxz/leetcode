   "source": [
    "from typing import List\n",
    "\n",
    "\n",
    "class Solution:\n",
    "    def minCostToMoveChips(self, position: List[int]) -> int:\n",
    "        tl = [0, 0]\n",
    "        for p in position:\n",
    "            tl[p % 2] += 1\n",
    "        return min(tl)"
   ]
  }
