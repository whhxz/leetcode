    "class Solution:\n",
    "    def rangeBitwiseAnd(self, m: int, n: int) -> int:\n",
    "        offset = 0\n",
    "        while m!=n:\n",
    "            m >>=1\n",
    "            n >>=1\n",
    "            offset += 1\n",
    "        return n << offset\n",
    "    \n",
    "            \n",
    "            "
   ]
